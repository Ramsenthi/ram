#include<iostream>
using namespace std;

int main()
{
    int a;
    cout<<"enter the number:";
    cin>>a;
    if(a%3==0&&a%5==0)
    {
        cout<<"fizz buzz";
    }
    else if(a%3==0)
    {
        cout<<"it is buzz";
    }
    else if(a%5==0)
    {
        cout<<"IT is buzz";
    }
    return 0;
   
}
